@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Employee - HUB</title>

    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" rel="stylesheet">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/css/all.min.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/css/sweetalert2.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/css/style.css">
</head>
<body>
    <div class="login__header">
        <h1 style="font-size:50px">EmployeeHUB</h1>
        <h1>HUB</h1>
    </div>
    <div class="loader-main hide-block">
        <div id="loading"></div>
    </div>
    <div class="login__wrap">
        <div class="login__box">
            <h3 class="text-center">You have successfully logged out from this application.</h3>
            <div class="form-group login__action">
                <a href="~/Login/Custom" class="btn btn-primary">Login</a>
            </div>
        </div>
    </div>

    <script src="~/js/jquery-3.6.0.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.validate.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.validate.unobtrusive.min.js" asp-append-version="true"></script>
    <!-- Bootstrap JS -->
    <script src="~/js/bootstrap.min.js" asp-append-version="true"></script>
    <environment include="Development">
        <script src="~/js/Common/ConfigURL.js" asp-append-version="true"></script>
    </environment>
    <script src="~/js/Common/RequestResponse.js" asp-append-version="true"></script>
    <script src="~/js/sweetalert2.all.js" asp-append-version="true"></script>
    <script src="~/js/Common/Common.js" asp-append-version="true"></script>

   
</body>
</html>
